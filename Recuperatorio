//2.	Construir una función FAC(n) para calcular el factorial de un entero
//Alvaro Torrico Suarez
#include <iostream>
int facto(int n);
using namespace std;

int main()
{
    int h,fact;
    cout<<"ingrese su numero: ";
    cin>>h;
    fact=facto(h);
    cout<<"el factorial es: "<<fact<<endl;
    return 0;
}
int facto(int n){
    int f=1;
    for (int i=1;i<=n;i++)
        f=f*i;
    return f;
}
